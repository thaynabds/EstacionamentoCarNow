Program.cs

using System;
using System.Collections.Generic;

class Program
{
    private static List<string> veiculos = new List<string>();
    private static decimal precoInicial = 5.00m;
    private static decimal precoPorHora = 2.00m;

    static void Main()
    {
        Console.WriteLine("Bem-vindo ao Sistema de Estacionamento!");
        Console.WriteLine("=======================================\n");
        
        MostrarMenu();
    }

    static void MostrarMenu()
    {
        bool continuar = true;
        
        while (continuar)
        {
            Console.WriteLine("\n=== MENU PRINCIPAL ===");
            Console.WriteLine("1 - Cadastrar ve√≠culo");
            Console.WriteLine("2 - Remover ve√≠culo");
            Console.WriteLine("3 - Listar ve√≠culos");
            Console.WriteLine("4 - Configurar pre√ßos");
            Console.WriteLine("5 - Sair");
            Console.Write("\nDigite sua op√ß√£o: ");
            
            string opcao = Console.ReadLine();
            
            switch (opcao)
            {
                case "1":
                    CadastrarVeiculo();
                    break;
                case "2":
                    RemoverVeiculo();
                    break;
                case "3":
                    ListarVeiculos();
                    break;
                case "4":
                    ConfigurarPrecos();
                    break;
                case "5":
                    continuar = false;
                    Console.WriteLine("\nSistema encerrado. At√© logo!");
                    break;
                default:
                    Console.WriteLine("\nOp√ß√£o inv√°lida! Tente novamente.");
                    break;
            }
        }
    }

    static void CadastrarVeiculo()
    {
        Console.Clear();
        Console.WriteLine("=== CADASTRAR VE√çCULO ===\n");
        Console.Write("Digite a placa do ve√≠culo: ");
        string placa = Console.ReadLine().ToUpper();
        
        if (string.IsNullOrWhiteSpace(placa))
        {
            Console.WriteLine("\nPlaca inv√°lida!");
            return;
        }
        
        if (veiculos.Contains(placa))
        {
            Console.WriteLine($"\nVe√≠culo com placa {placa} j√° est√° estacionado!");
            return;
        }
        
        veiculos.Add(placa);
        Console.WriteLine($"\n‚úì Ve√≠culo {placa} cadastrado com sucesso!");
        Console.WriteLine($"Hora de entrada: {DateTime.Now:HH:mm}");
    }

    static void RemoverVeiculo()
    {
        Console.Clear();
        Console.WriteLine("=== REMOVER VE√çCULO ===\n");
        
        if (veiculos.Count == 0)
        {
            Console.WriteLine("N√£o h√° ve√≠culos estacionados para remover.");
            return;
        }
        
        Console.Write("Digite a placa do ve√≠culo: ");
        string placa = Console.ReadLine().ToUpper();
        
        if (!veiculos.Contains(placa))
        {
            Console.WriteLine($"\nVe√≠culo com placa {placa} n√£o encontrado!");
            Console.WriteLine("Confira a lista de ve√≠culos estacionados.");
            return;
        }
        
        Console.Write("Quantas horas o ve√≠culo permaneceu estacionado? ");
        
        if (!int.TryParse(Console.ReadLine(), out int horas) || horas < 0)
        {
            Console.WriteLine("\nQuantidade de horas inv√°lida!");
            return;
        }
        
        decimal valorTotal = precoInicial + (precoPorHora * horas);
        
        Console.WriteLine("\n" + new string('=', 40));
        Console.WriteLine("RECIBO DE ESTACIONAMENTO");
        Console.WriteLine(new string('=', 40));
        Console.WriteLine($"Placa do ve√≠culo: {placa}");
        Console.WriteLine($"Hora de sa√≠da: {DateTime.Now:HH:mm}");
        Console.WriteLine($"Tempo estacionado: {horas} hora(s)");
        Console.WriteLine($"Pre√ßo inicial: R$ {precoInicial:F2}");
        Console.WriteLine($"Pre√ßo por hora: R$ {precoPorHora:F2}");
        Console.WriteLine(new string('-', 40));
        Console.WriteLine($"VALOR TOTAL: R$ {valorTotal:F2}");
        Console.WriteLine(new string('=', 40));
        
        veiculos.Remove(placa);
        Console.WriteLine($"\n‚úì Ve√≠culo {placa} removido com sucesso!");
    }

    static void ListarVeiculos()
    {
        Console.Clear();
        Console.WriteLine("=== VE√çCULOS ESTACIONADOS ===\n");
        
        if (veiculos.Count == 0)
        {
            Console.WriteLine("Nenhum ve√≠culo estacionado no momento.");
            return;
        }
        
        Console.WriteLine($"Total de ve√≠culos: {veiculos.Count}\n");
        
        for (int i = 0; i < veiculos.Count; i++)
        {
            Console.WriteLine($"{i + 1}. {veiculos[i]}");
        }
    }

    static void ConfigurarPrecos()
    {
        Console.Clear();
        Console.WriteLine("=== CONFIGURAR PRE√áOS ===\n");
        
        Console.WriteLine($"Pre√ßo atual inicial: R$ {precoInicial:F2}");
        Console.Write("Novo pre√ßo inicial: R$ ");
        
        if (decimal.TryParse(Console.ReadLine(), out decimal novoInicial) && novoInicial >= 0)
        {
            precoInicial = novoInicial;
            Console.WriteLine($"‚úì Pre√ßo inicial atualizado para R$ {precoInicial:F2}");
        }
        else
        {
            Console.WriteLine("Valor inv√°lido! Pre√ßo n√£o alterado.");
        }
        
        Console.WriteLine($"\nPre√ßo atual por hora: R$ {precoPorHora:F2}");
        Console.Write("Novo pre√ßo por hora: R$ ");
        
        if (decimal.TryParse(Console.ReadLine(), out decimal novoHora) && novoHora >= 0)
        {
            precoPorHora = novoHora;
            Console.WriteLine($"‚úì Pre√ßo por hora atualizado para R$ {precoPorHora:F2}");
        }
        else
        {
            Console.WriteLine("Valor inv√°lido! Pre√ßo n√£o alterado.");
        }
    }
}

